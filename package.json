{
  "name": "ariadne",
  "version": "2.8.4",
  "description": "Automated Productivity Helper",
  "main": "ariadne.js",
  "private": true,
  "engines": {
    "node": ">=6"
  },
  "scripts": {
    "eslint": "eslint . --ext .js",
    "fix": "eslint --fix . --ext .js",
    "test": "mocha -R spec test",
    "start": "pm2 start .pm2.json",
    "reload": "./tools/reload-server.sh",
    "dev": "nodemon ariadne.js --config .nodemon.json",
    "preversion": "git checkout master && git merge --no-ff develop -m 'Pull changes for release bump'",
    "postversion": "git push && git push --tags && git checkout develop && git merge --no-ff master -m 'Sync Develop branch' && git push",
    "dist": "./tools/upgrade.sh",
    "coverage": "istanbul cover node_modules/.bin/_mocha -- -R spec test && xdg-open ./coverage/lcov-report/index.html",
    "doc": "node_modules/.bin/jsdoc -t jaguarjs-jsdoc -c .jsdoc.json && xdg-open ./docs/index.html"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DaJoker29/ariadne.git"
  },
  "author": "Dewitt Buckingham (http://zerodaedalus.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/DaJoker29/ariadne/issues"
  },
  "homepage": "https://github.com/DaJoker29/ariadne#readme",
  "dependencies": {
    "eslint": "3.9.1",
    "github": "6.0.4",
    "graceful-fs": "4.1.10",
    "jsdoc": "3.4.3",
    "lodash": "4.16.6",
    "mathjs": "3.7.0",
    "mocha": "3.1.2",
    "moment": "2.16.0",
    "mongoose": "4.6.7",
    "node-schedule": "1.2.0",
    "nodemailer": "2.6.4",
    "twitter": "1.4.0"
  },
  "devDependencies": {
    "eslint": "3.9.1",
    "eslint-config-airbnb": "13.0.0",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jsx-a11y": "2.2.3",
    "eslint-plugin-react": "6.6.0"
  }
}
