{
  "name": "ariadne",
  "version": "2.0.10",
  "description": "Automated Productivity Helper",
  "main": "ariadne.js",
  "private": true,
  "engines": {
    "node": ">=6"
  },
  "scripts": {
    "eslint": "eslint . --ext .js",
    "fix": "eslint --fix . --ext .js",
    "test": "mocha -R nyan test",
    "start": "NODE_ENV=production pm2 start ariadne.js --name ariadne",
    "reload": "pm2 reload all",
    "dev": "nodemon --config config/nodemon.json ariadne.js",
    "preversion": "git checkout master && git merge --no-ff develop -m 'Pull changes for release bump'",
    "postversion": "git push && git push --tags && git checkout develop && git merge --no-ff master -m 'Sync Develop branch' && git push",
    "dist": "./scripts/shell/upgrade.sh"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DaJoker29/ariadne.git"
  },
  "author": "Dewitt Buckingham (http://zerodaedalus.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/DaJoker29/ariadne/issues"
  },
  "homepage": "https://github.com/DaJoker29/ariadne#readme",
  "dependencies": {
    "eslint": "3.9.1",
    "graceful-fs": "4.1.10",
    "mocha": "3.1.2",
    "node-schedule": "1.2.0",
    "nodemailer": "2.6.4"
  },
  "devDependencies": {
    "eslint": "3.9.1",
    "eslint-config-airbnb": "13.0.0",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jsx-a11y": "2.2.3",
    "eslint-plugin-react": "6.6.0"
  }
}
